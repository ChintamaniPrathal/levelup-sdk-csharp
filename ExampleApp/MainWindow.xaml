<Window x:Class="ConfigurationTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:tabData="clr-namespace:ConfigurationTool"
        Title=". : LevelUp Configuration Tool" 
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanMinimize"
        Height="375" 
        Width="420">
    <Grid Margin="0,0,0,0" Background="LightGray">
        <TabControl Margin="0,0,0,0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
            <tabData:LevelUpTabItem />
            <tabData:TestTabItem />
        </TabControl>
    </Grid>
</Window>
